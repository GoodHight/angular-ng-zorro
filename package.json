{
  "name": "ng-alain",
  "version": "1.0.3",
  "description": "ng-zorro-antd admin panel front-end framework",
  "author": "cipchk",
  "homepage": "https://cipchk.github.io/ng-alain/",
  "bugs": {
    "url": "https://github.com/cipchk/ng-alain/issues"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 3.0.0"
  },
  "license": "MIT",
  "scripts": {
    "precommit": "npm run lint:ts",
    "ng": "ng",
    "rimraf": "rimraf",
    "start": "ng serve -o",
    "build": "ng build --prod --build-optimizer",
    "build:test": "npm run clean:dist && ng build --c=test --prod --build-optimizer",
    "build:prod": "npm run clean:dist && ng build --c=production --prod --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test -sr -sm=false",
    "test-coverage": "ng test --code-coverage --watch=false",
    "clean:dist": "npm run rimraf -- dist",
    "clean:bundles": "npm run rimraf -- bundles",
    "el:start": "npm run el:build-ng && npm run build-tasks && npm run el:build && npm run el:start-app",
    "el:watch": "npm run build-tasks && npm run el:build && gulp build.default && npm run el:start-app",
    "el:build-ng": "ng build",
    "el:build-ng-test": "npm run clean:bundles && npm run build:test",
    "el:build-ng-prod": "npm run clean:bundles && npm run build",
    "el:build": "gulp build.electron",
    "el:start-app": "electron dist/electron-app.js",
    "el:bundle.clean": "gulp bundle.clean",
    "el:bundle.prepare.test": "npm run el:build-ng-test && npm run build-tasks && npm run el:build && npm run el:bundle.prepare.npm && npm run el:bundle.clean",
    "el:bundle.prepare": "npm run el:build-ng-prod && npm run build-tasks && npm run el:build && npm run el:bundle.prepare.npm && npm run el:bundle.clean",
    "el:bundle.prepare.npm": "gulp build.npm.copy",
    "el:bundle.mac.test": "npm run el:bundle.prepare.test && electron-packager dist/ hrm-manager --platform=darwin --arch=x64 --overwrite --out=bundles",
    "el:bundle.windows.test": "npm run el:bundle.prepare.test && electron-packager dist/ hrm-manager --platform=win32 --overwrite --out=bundles",
    "el:bundle.linux.test": "npm run el:bundle.prepare.test && electron-packager dist/ hrm-manager --platform=linux --overwrite --out=bundles",
    "el:bundle.mac": "npm run el:bundle.prepare && electron-packager dist/ hrm-manager --platform=darwin --arch=x64 --overwrite --out=bundles",
    "el:bundle.windows": "npm run el:bundle.prepare && electron-packager dist/ hrm-manager --platform=win32 --overwrite --out=bundles",
    "el:bundle.linux": "npm run el:bundle.prepare && electron-packager dist/ hrm-manager --platform=linux --overwrite --out=bundles",
    "el:packager": "electron-packager"
  },
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "@delon/abc": "1.0.3",
    "@delon/acl": "1.0.3",
    "@delon/auth": "1.0.3",
    "@delon/cache": "1.0.3",
    "@delon/form": "1.0.3",
    "@delon/mock": "1.0.3",
    "@delon/theme": "1.0.3",
    "@delon/util": "1.0.3",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/html2canvas": "0.0.33",
    "angular-sortablejs": "^2.5.2",
    "core-js": "^2.5.4",
    "electron": "^2.0.0",
    "electron-connect": "^0.6.3",
    "electron-packager": "^12.0.2",
    "eslint": "^4.19.1",
    "file-saver": "^1.3.8",
    "html2canvas": "^1.0.0-alpha.12",
    "moment": "^2.22.1",
    "ng-zorro-antd": "0.7.0",
    "ngx-countdown": "^3.0.0",
    "ngx-tinymce": "^2.0.0",
    "ngx-ueditor": "^2.0.0",
    "qrcode": "^1.2.2",
    "qrious": "^4.0.2",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.1.0",
    "screenfull": "^3.3.1",
    "simple-line-icons": "^2.4.1",
    "sortablejs": "^1.7.0",
    "ts-md5": "^1.2.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.0",
    "@angular-devkit/build-angular": "~0.6.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/jszip": "^3.1.3",
    "@types/mockjs": "^1.0.0",
    "codecov": "^3.0.2",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "editorconfig-tools": "^0.1.1",
    "husky": "^0.14.3",
    "gh-pages": "^1.1.0",
    "lint-staged": "^7.1.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack-bundle-analyzer": "^2.11.2",
    "xlsx": "^0.12.12",
    "less": "~2.7.0",
    "ng-alain": "^1.0.1",
    "@angularclass/hmr": "^2.1.3",
    "electron-debug": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.6.1",
    "gulp-tsc": "^1.3.2",
    "gulp-inject-string": "^1.1.1",
    "rimraf": "^2.6.1"
  },
  "lint-staged": {
    "src/**/*.ts": "lint:ts",
    "src/**/*.less": "lint:style"
  }
}